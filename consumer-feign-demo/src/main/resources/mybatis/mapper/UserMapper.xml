<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.guoyuan.springboot.dao.UserDao">

	<select id="loginAuthorization" resultType="User" parameterType="String">
		select userName,userPwd from tb_guoyuan_user where userName=#{userName};
	</select>
	

</mapper>
 

